<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_class_member"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.classcircle.activity.MyClassMemberActivity">

    <include layout="@layout/layout_headed"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/layout_loading_progress"
            android:id="@+id/loading_layout"/>
    <android.support.v7.widget.RecyclerView
        android:layout_below="@id/loading_layout"
        android:id="@+id/id_class_member_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></android.support.v7.widget.RecyclerView>


    </RelativeLayout>
</LinearLayout>
